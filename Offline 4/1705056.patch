+void clear_reference_bits(struct proc *p)
+{
+  if (!PAGE_REPLACEMENT)
+  {
+    panic("Inside clear_reference_bits wrong algorithm entered!");
+  }
+  for (int i = 0; i < MAX_PSYC_PAGES; i++)
+  {
+    if (p->nru_physical_mem.a[i].in_use == 0)
