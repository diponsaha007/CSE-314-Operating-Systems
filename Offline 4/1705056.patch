+{
+  return a->cur_size < a->max_size;
+}
+
+void inc_start(struct FifoQueue *a)
+{
+  a->l = (a->l + 1) % a->max_size;
+}
+
+void inc_end(struct FifoQueue *a)
+{
+  a->r = (a->r + 1) % a->max_size;
+}
+
+void add_size(struct FifoQueue *a, int val)
+{
+  a->cur_size += val;
+}
+
+void update_end(struct FifoQueue *a)
+{
+  a->r = (a->l + a->cur_size) % a->max_size;
+}
+
+void shift_fifo(struct FifoQueue *p, int start)
+{
+  p->cur_size--;
+  int i = start;
+  while (1)
+  {
+    int j = (i + 1) % MAX_PSYC_PAGES;
+    if (j == p->r)
+      break;
+    p->a[i] = p->a[j];
+    i = j;
+  }
+  update_end(p);
+}
+
+void delete_from_queue(struct FifoQueue *p, uint va)
+{
+  for (int i = p->l, cnt = 0; cnt < p->cur_size; i = (i + 1) % MAX_PSYC_PAGES, cnt++)
+  {
+    if (p->a[i] == va)
+    {
+      shift_fifo(p, i);
+      break;
+    }
+  }
