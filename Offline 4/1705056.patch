+int nru_replacement_algo(struct proc *p)
+{
+  if (!PAGE_REPLACEMENT)
+  {
+    panic("Inside nru_replacement_algo wrong algorithm entered!");
+  }
+  int c[4] = {-1, -1, -1, -1};
+  for (int i = 0; i < MAX_PSYC_PAGES; i++)
+  {
+    if (p->nru_physical_mem.a[i].in_use == 0)
+      continue;
+    uint va = p->nru_physical_mem.a[i].va;
+    pte_t *pte;
+    pte = walkpgdir(p->pgdir, (char *)va, 0);
+    if (!pte)
+    {
+      panic("Inside nru_replacement_algo pte not found!");
+    }
+    int x1 = ((*pte) & PTE_A);
+    int x2 = ((*pte) & PTE_D);
+    if (x1 && x2)
+    {
+      c[0] = i;
+    }
+    else if (!x1 && x2)
+    {
+      c[1] = i;
+    }
+    else if (x1 && !x2)
+    {
+      c[2] = i;
+    }
+    else
+    {
+      c[3] = i;
+    }
+  }
+  for (int i = 0; i < 4; i++)
+  {
+    if (c[i] != -1)
+    {
+      return c[i];
+    }
+  }
+  panic("Inside nru_replacement_algo no index found!");
+}
+
+void insert_to_physical_memory(struct proc *p, uint va)
+{
+  if (!PAGE_REPLACEMENT)
+  {
+    int idx = (has_empty_spot(&p->physical_mem) ? p->physical_mem.r : -1);
+    if (idx == -1)
+    {
+      idx = p->physical_mem.l;
+      cprintf("FIFO --> Index Got = %d, Page Number = %d\n", idx, (int)p->physical_mem.a[idx] / PGSIZE);
+      swap_out_page(idx, p);
+    }
+    insert_to_fifo(&p->physical_mem, va);
+  }
+  else
+  {
+    int idx = nru_get_empty_spot(&p->nru_physical_mem);
+    if (idx == -1)
+    {
+      idx = nru_replacement_algo(p);
+      cprintf("NRU --> Index Got = %d, Page Number = %d\n", idx, (int)p->nru_physical_mem.a[idx].va / PGSIZE);
+      swap_out_page(idx, p);
+    }
+    nru_insert_idx(&p->nru_physical_mem, va, idx);
+  }
+}
+
+void swap_out_page(int idx, struct proc *p)
+{
+  // cprintf("Swapping out page of process %d at index %d\n", p->pid, idx);
+  if (!PAGE_REPLACEMENT)
+  {
+    pte_t *pte = walkpgdir(p->pgdir, (int *)p->physical_mem.a[idx], 0);
+    if (!pte)
+    {
+      panic("Inside Swap_out_page could not found the page.\n");
+    }
+    move_to_swapfile(p, p->physical_mem.a[idx]);
+    add_size(&p->physical_mem, -1);
+    inc_start(&p->physical_mem);
+    char *va = (char *)P2V(PTE_ADDR(*pte));
+    // update the page table entry flags
+    *pte &= (~PTE_P);
+    *pte |= PTE_PG;
+    *pte &= PTE_FLAGS(*pte);
+    lcr3(V2P(p->pgdir));
+    // freeing the virtual address
+    kfree(va);
+  }
+  else
+  {
+    pte_t *pte = walkpgdir(p->pgdir, (int *)p->nru_physical_mem.a[idx].va, 0);
+    if (!pte)
+    {
+      panic("Inside Swap_out_page could not found the page.\n");
+    }
+    move_to_swapfile(p, p->nru_physical_mem.a[idx].va);
+    nru_add_size(&p->nru_physical_mem, -1);
+    p->nru_physical_mem.a[idx].in_use = 0;
+    char *va = (char *)P2V(PTE_ADDR(*pte));
+    // update the page table entry flags
+    *pte &= (~PTE_P);
+    *pte |= PTE_PG;
+    *pte &= PTE_FLAGS(*pte);
+    lcr3(V2P(p->pgdir));
+    // freeing the virtual address
+    kfree(va);
+  }
+}
 
+void move_to_swapfile(struct proc *p, uint virtual_add)
+{
+  int idx = -1;
+  for (int i = 0; i < MAX_DISK_PAGES; i++)
+  {
+    if (p->disk_mem[i].in_use)
+      continue;
+    idx = i;
+    break;
+  }
+  if (idx == -1)
+  {
+    panic("Inside move_to_swapfile! index not found!");
+  }
+  p->disk_mem[idx].va = virtual_add;
+  p->disk_mem[idx].in_use = 1;
+  int x = writeToSwapFile(p, (char *)virtual_add, PGSIZE * idx, PGSIZE);
+  if (x == -1)
+  {
+    panic("Inside move_to_swapfile. Write to swap file failed!\n");
+  }
+}
+
+void update_new_flags(struct proc *p, uint phy_addr, uint vir_addr)
+{
+  pte_t *pte = walkpgdir(p->pgdir, (int *)vir_addr, 0);
+  if (!pte)
+  {
+    panic("Inside update_new_flags error 1 PTE not found!\n");
+  }
+  if ((*pte & PTE_P))
+  {
+    panic("Inside update_new_flags error 2 flag not present!\n");
+  }
+  *pte &= (~PTE_PG);
+  *pte |= (PTE_P | PTE_W | PTE_U | phy_addr);
+
+  lcr3(V2P(p->pgdir));
+}
+
+void move_to_memory(struct proc *p, int index, uint virtual_address, char *buff)
+{
+  for (int i = 0; i < MAX_DISK_PAGES; i++)
+  {
+    if (p->disk_mem[i].va == virtual_address)
+    {
+      int x = readFromSwapFile(p, buff, i * PGSIZE, PGSIZE);
+      if (x == -1)
+      {
+        panic("Inside move_to_memory. Read from swap file failed.\n");
+      }
+
+      if (!PAGE_REPLACEMENT)
+      {
+        p->physical_mem.a[index] = p->disk_mem[i].va;
+      }
+      else
+      {
+        p->nru_physical_mem.a[index].va = p->disk_mem[i].va;
+        p->nru_physical_mem.a[index].in_use = 1;
+      }
+      p->disk_mem[i].in_use = 0;
+      return;
+    }
+  }
+  panic("Inside move_to_memory. Virtual address not found!");
+}
+
+void swap_in_page(struct proc *p, uint virtual_address)
+{
+  // cprintf("Inside swap_in_page %d\n", virtual_address);
+  virtual_address = PGROUNDDOWN(virtual_address);
+  char *new_page = kalloc();
+  memset(new_page, 0, PGSIZE);
+  update_new_flags(p, V2P(new_page), virtual_address);
+
+  if (!PAGE_REPLACEMENT)
+  {
+    int index = (has_empty_spot(&p->physical_mem) ? p->physical_mem.r : -1);
+    if (index != -1)
+    {
+      move_to_memory(p, index, virtual_address, (char *)virtual_address);
+      add_size(&p->physical_mem, 1);
+      inc_end(&p->physical_mem);
+      return;
+    }
+
+    index = p->physical_mem.l;
+    cprintf("FIFO --> Index Got = %d, Page Number = %d\n", index, (int)p->physical_mem.a[index] / PGSIZE);
+    uint bad = p->physical_mem.a[index];
+    pte_t *pte_t = walkpgdir(p->pgdir, (int *)bad, 0);
+    int badpa = PTE_ADDR(*pte_t);
+
+    move_to_memory(p, index, virtual_address, new_page);
+    move_to_swapfile(p, bad);
+    *pte_t &= (~PTE_P);
+    *pte_t |= PTE_PG;
+    *pte_t &= PTE_FLAGS(*pte_t);
+    lcr3(V2P(p->pgdir));
+    inc_start(&p->physical_mem);
+    inc_end(&p->physical_mem);
+    kfree((char *)P2V(badpa));
+  }
+  else
+  {
+    int index = nru_get_empty_spot(&p->nru_physical_mem);
+    if (index != -1)
+    {
+      move_to_memory(p, index, virtual_address, (char *)virtual_address);
+      nru_add_size(&p->nru_physical_mem, 1);
+      return;
+    }
+
+    index = nru_replacement_algo(p); // todo
+    cprintf("NRU --> Index Got = %d, Page Number = %d\n", index, (int)p->nru_physical_mem.a[index].va / PGSIZE);
+
+    uint bad = p->nru_physical_mem.a[index].va;
+    pte_t *pte_t = walkpgdir(p->pgdir, (int *)bad, 0);
+    int badpa = PTE_ADDR(*pte_t);
+    move_to_memory(p, index, virtual_address, new_page);
+    move_to_swapfile(p, bad);
+    *pte_t &= (~PTE_P);
+    *pte_t |= PTE_PG;
+    *pte_t &= PTE_FLAGS(*pte_t);
+    lcr3(V2P(p->pgdir));
+    kfree((char *)P2V(badpa));
+  }
+}
+
+int is_page_in_swapfile(struct proc *p, int va)
+{
+  // cprintf("Inside page_in_swapfile %d\n", va);
